var x{1..3} binary; # Declare 6 integer variables

#Alternative syntax
# var x1 binary;
# var x2 binary;
# var x3 binary;

minimize obj: 0; 
#Alternatively minimize obj: 0

c1: x[1] + x[2] + (1 - x[3]) >= 1; # Alternatively: x1 + x2 + (1 - x3) >= 1
c2: x[1] + x[2] + x[3] >= 1;
c3: x[1] + (1 - x[2]) + (1 -x[3]) >= 1;
c4: x[1] + + (1 - x[2]) +  x[3]  >= 1;
c5: 1 - x[1] >= 1;


solve;

display{i in 1..3} x[i]; # alternative syntax display x1, x2,x3, x4, x5, x6;


end;
